{
  "name": "home-improvement-journey",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.2.1",
    "@kadira/storybook": "2.5.2",
    "@kadira/storybook-addon-knobs": "1.2.3",
    "flow-bin": "0.33.0",
    "react-scripts": "0.6.1",
    "sw-precache": "4.1.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "npm run clean && react-scripts build && npm run service-worker",
    "clean": "rm -fr build/*",
    "eject": "react-scripts eject",
    "flow": "flow",
    "start": "react-scripts start",
    "service-worker": "cp manifest.json build/ && sw-precache --root='build/' --static-file-globs='build/**/!(*map*)'",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook",
    "test": "react-scripts test --env=jsdom"
  },
  "homepage": "./"
}
